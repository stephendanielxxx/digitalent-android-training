<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.digitalenttraining">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DigitalentTraining">
        <activity android:name=".APIActivity"></activity>
        <activity android:name=".HorizontalListActivity" />
        <activity android:name=".RecyclerViewActivity" />
        <activity android:name=".TrainingListViewActivity" />
        <activity android:name=".ListViewActivity" />
        <activity android:name=".ListActivity" />
        <activity android:name=".ActivityTwo" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityThree" />
    </application>

</manifest>